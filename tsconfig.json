{
    "compileOnSave": false,
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": false,
        "declaration": false,
        "moduleResolution": "Node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "target": "es2017",
        "lib": [
            "esnext"
        ],
        "baseUrl": "src/",
        "paths": {},
        "module": "esnext",
        "strict": true,
        "noImplicitReturns": true,
        "allowSyntheticDefaultImports": true,
        "allowUnreachableCode": false
    }
}